        "PLC_ADDR":[
            "plc에서 읽을 주소의 목록",
            "그룹명을 key로 하는 딕셔너리",
            "주소 이름과 주소+옵션을 매핑",
            {
                "옵션":"주소값 뒤에 #이후 5글자",
                "옵션 없음": "#00000",
                "옵션[0:2]":"byte 크기",
                "옵션[2]":{
                    "값 형식":["I","i","F","f","C"],
                    "I":"unsigned int"
                },
                "옵션[3:5]":{
                    "각 형식별 옵션":[],
                    "I":""
                }
            }
        ],
        "TABLE_DATA":[
